{
  "compileOnSave": false,
  "compilerOptions": {
    "rootDir": ".",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es2015",
    "module": "esnext",
    "lib": ["es2017", "dom"],
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "@ngmug/ngmug/feature-about-ngmug": [
        "libs/ngmug/feature-about-ngmug/src/index.ts"
      ],
      "@ngmug/ngmug/feature-components": [
        "libs/ngmug/feature-components/src/index.ts"
      ],
      "@ngmug/ngmug/feature-rxjs-helpers": [
        "libs/ngmug/feature-rxjs-helpers/src/index.ts"
      ],
      "@ngmug/ngmug/feature-shell-ngmug": [
        "libs/ngmug/feature-shell-ngmug/src/index.ts"
      ],
      "@ngmug/shared/gh-headless-ui": [
        "libs/shared/gh-headless-ui/src/index.ts"
      ],
      "@ngmug/shared/gh-layout": ["libs/shared/gh-layout/src/index.ts"],
      "@ngmug/shared/gh-utils": ["libs/shared/gh-utils/src/index.ts"],
      "@ngmug/shared/ngmug-utils": ["libs/shared/ngmug-utils/src/index.ts"],
      "@ngmug/trip-calendar/feature-calendar": [
        "libs/trip-calendar/feature-calendar/src/index.ts"
      ],
      "@ngmug/trip-calendar/feature-shell-tc": [
        "libs/trip-calendar/feature-shell-tc/src/index.ts"
      ],
      "@ngmug/trip-calendar/ui-tc": ["libs/trip-calendar/ui-tc/src/index.ts"]
    }
  },
  "exclude": ["node_modules", "tmp"]
}
